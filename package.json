{
    "main ": "./dist/ index.js ",
    "bin": {
        "t": "./dist/index.js"
    },
    "scripts": {
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
        "create": "npm run build && npm run test",
        "build": "tsc -p .",
        "local": "sudo npm i -g && t",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "clear": "^0.1.0",
        "cli-progress": "^3.10.0",
        "dotenv": "^16.0.0",
        "figlet": "^1.5.2",
        "inquirer": "^8.2.1",
        "path": "^0.12.7",
        "toggl-api": "^1.0.2"
    },
    "devDependencies": {
        "@types/clear": "^0.1.2",
        "@types/figlet": "^1.5.4",
        "@types/node": "^17.0.22",
        "nodemon": "^2.0.15",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2"
    }
}